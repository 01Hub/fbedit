   66:				;SUBROUTINES
   67:				; MSGC sends the data pointed to by
   68:				; the DPTR to the graphics module
   69:				; as a series of commands with
   70:				; two parameters each.
   71:	  016C			MSGC:
   72:	  016C	78 02			MOV	R0,#2		;# of data bytes
   73:	  016E			MSGC2:
   74:	  016E	E4			CLR	A
   75:	  016F	93			MOVC	A,@A+DPTR	;get byte
   76:	  0170	B4 A1 01		CJNE	A,#0A1h,MSGC3	;done?
   77:	  0173	22			RET
   78:
   79:	  0174			MSGC3:
   80:	  0174	F9			MOV	R1,A
   81:	  0175	12 01 AD		LCALL	WRITED		;send it
   82:	  0178	A3			INC	DPTR
   83:	  0179	D8 F3			DJNZ	R0,MSGC2
   84:	  017B	E4			CLR	A
   85:	  017C	93			MOVC	A,@A+DPTR	;get command
   86:	  017D	F9			MOV	R1,A
