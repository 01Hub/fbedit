   44:				;SUBROUTINES
   45:				; MSGC sends the data pointed to by
   46:				; the DPTR to the graphics module
   47:				; as a series of commands with
   48:				; two parameters each.
   49:	  0133			MSGC:
   50:	  0133	78 02			MOV	R0,#2		;# of data bytes
   51:	  0135			MSGC2:
   52:	  0135	E4			CLR	A
   53:	  0136	93			MOVC	A,@A+DPTR	;get byte
   54:	  0137	B4 A1 01		CJNE	A,#0A1h,MSGC3	;done?
   55:	  013A	22			RET
   56:
   57:	  013B			MSGC3:
   58:	  013B	F9			MOV	R1,A
   59:	  013C	12 01 74		LCALL	WRITED		;send it
   60:	  013F	A3			INC	DPTR
   61:	  0140	D8 F3			DJNZ	R0,MSGC2
   62:	  0142	E4			CLR	A
   63:	  0143	93			MOVC	A,@A+DPTR	;get command
   64:	  0144	F9			MOV	R1,A
   65:	  0145	12 01 68		LCALL	WRITEC		;send command
   66:	  0148	A3			INC	DPTR
   67:	  0149	80 E8			SJMP	MSGC		;next command
   68:
   69:				; MSGD sends the data pointed to by
   70:				; the DPTR to the graphics module.
   71:	  014B			MSGD:
   72:	  014B	E4			CLR	A
   73:	  014C	93			MOVC	A,@A+DPTR	;get byte
   74:	  014D	B4 A1 01		CJNE	A,#0A1h,MSGD1	;done?
   75:	  0150	22			RET
   76:
   77:	  0151			MSGD1:
   78:	  0151	F9			MOV	R1,A
   79:	  0152	12 01 74		LCALL	WRITED		;send data
   80:	  0155	A3			INC	DPTR
   81:	  0156	80 F3			SJMP	MSGD
   82:
   83:				; MSGDT sends the data pointed to by
   84:				; the DPTR to the graphics module.
   85:				;20h is subtracted to get the psaudo ascii
   86:	  0158			MSGDT:
   87:	  0158	E4			CLR	A
   88:	  0159	93			MOVC	A,@A+DPTR	;get byte
   89:	  015A	B4 A1 01		CJNE	A,#0A1h,MSGDT1	;done?
   90:	  015D	22			RET
   91:
   92:	  015E			MSGDT1:
   93:	  015E	C3			CLR	C
   94:	  015F	94 20			SUBB	A,#20h		;Subtract 20h to get psaudo ascii
   95:	  0161	F9			MOV	R1,A
   96:	  0162	12 01 74		LCALL	WRITED		;send data
   97:	  0165	A3			INC	DPTR
   98:	  0166	80 F0			SJMP	MSGDT
   99:

