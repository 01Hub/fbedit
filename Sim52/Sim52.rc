IDM_MENU MENUEX
BEGIN
  POPUP "&File",IDM_FILE
  BEGIN
    MENUITEM "&New Project",IDM_FILE_NEWPROJECT
    MENUITEM "",,0x00000800
    MENUITEM "&Open File / Project\tCtrl+O",IDM_FILE_OPENFILE
    MENUITEM "",,0x00000800
    MENUITEM "Close File / Project",IDM_FILE_CLOSE
    MENUITEM "",,0x00000800
    MENUITEM "E&xit",IDM_FILE_EXIT
  END
  POPUP "&Search",IDM_SEARCH
  BEGIN
    MENUITEM "&Find\tCtrl+F",IDM_SEARCH_FIND
  END
  POPUP "&View",IDM_VIEW
  BEGIN
    MENUITEM "&Terminal",IDM_VIEW_TERMINAL
  END
  POPUP "&Debug",IDM_DEBUG
  BEGIN
    MENUITEM "&Run\tShift+F7",IDM_DEBUG_RUN
    MENUITEM "&Pause\tCtrl+Alt+F7",IDM_DEBUG_PAUSE
    MENUITEM "&Stop\tAlt+F7",IDM_DEBUG_STOP
    MENUITEM "Step &Into\tF7",IDM_DEBUG_STEP_INTO
    MENUITEM "Step &Over\tCtrl+F7",IDM_DEBUG_STEP_OVER
    MENUITEM "Run to &Cursor\tShift+Ctrl+F7",IDM_DEBUG_RUN_TO_CURSOR
    MENUITEM "&Toggle Breakpoint\tCtrl+T",IDM_DEBUG_TOGGLE
    MENUITEM "Clear &All Breakpoints\tShift+Ctrl+T",IDM_DEBUG_CLEAR
  END
  POPUP "&Options",IDM_OPTIONS
  BEGIN
    POPUP "MCU",IDM_OPTION_MCU
    BEGIN
      MENUITEM "(Empty)",IDM_MCU,,0x00000003
    END
    MENUITEM "&Clocks && Refresh",IDM_OPTION_CLOCK
  END
  POPUP "&Tools",IDM_TOOLS
  BEGIN
    MENUITEM "Clear &Ram",IDM_TOOLS_CLEARRAM
    MENUITEM "Clear &XRam",IDM_TOOLS_CLEARXRAM
  END
  POPUP "&Help",IDM_HELP
  BEGIN
    MENUITEM "&About",IDM_HELP_ABOUT
    MENUITEM "",,0x00000800
    MENUITEM "(Empty)",IDM_HELP_EMPTY,,0x00000003
  END
END

