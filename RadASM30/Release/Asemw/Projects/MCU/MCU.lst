 3730:                          ;OUT:   32 Bit result in R7:R6:R5:R4
 3731:                          ;------------------------------------------------------------------
 3732:    3DBC  44 7C           FRQCOUNT:       ORL     A,#7Ch                          ;D0,D1  CHANNEL (0-3)
 3733:                                                                                  ;D2     FRQ     Gate active low
 3734:                                                                                  ;D3     FRQ     Reset active high
 3735:                                                                                  ;D4     ADC     CS Active low
 3736:                                                                                  ;D5     ADC     CLK High to Low transition
 3737:                                                                                  ;D6     ADC     DIN
 3738:                                                                                  ;D7     FRQ TTL  Active high
 3739:    3DBE  90 80 01                        MOV     DPTR,#8001h
 3740:    3DC1  F0                              MOVX    @DPTR,A                         ;Reset and gate off
 3741:    3DC2  C0 E0                           PUSH    ACC
 3742:    3DC4  75 8A 00                        MOV     TL0,#00h
 3743:    3DC7  75 8C 00                        MOV     TH0,#00h
 3744:    3DCA  E5 89                           MOV     A,TMOD
 3745:    3DCC  D2 E0                           SETB    ACC.0                           ;M00
 3746:    3DCE  C2 E1                           CLR     ACC.1                           ;M01
 3747:    3DD0  D2 E2                           SETB    ACC.2                           ;C/T0#
 3748:    3DD2  C2 E3                           CLR     ACC.3                           ;GATE0
 3749:    3DD4  F5 89                           MOV     TMOD,A
 3750:    3DD6  E5 88                           MOV     A,TCON
 3751:    3DD8  D2 E4                           SETB    ACC.4                           ;TR0
 3752:    3DDA  C2 E5                           CLR     ACC.5                           ;TF0
 3753:    3DDC  F5 88                           MOV     TCON,A
 3754:    3DDE  D0 E0                           POP     ACC
 3755:    3DE0  54 F3                           ANL     A,#0F3h                         ;D2     FRQ Gate active low
 3756:                                                                                  ;D3     FRQ Reset active high
 3757:    3DE2  F0                              MOVX    @DPTR,A                         ;Gate on(low),Reset inactive (low)
 3758:    3DE3  B1 AF                           ACALL   WAITASEC
 3759:    3DE5  D2 E2                           SETB    ACC.2                           ;D2     FRQ Gate active low
 3760:    3DE7  F0                              MOVX    @DPTR,A                         ;Stop counting
