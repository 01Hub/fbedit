#define IDM_PROJECT 10042
#define IDM_MAKE 10036
#define IDM_MAKE_ASSEMBLE 10037
#define IDM_MAKE_BUILD 10038
#define IDM_MAKE_RUN 10039
#define IDM_MAKE_GO 10040
#define IDM_TOOLS 10043
#define IDM_WINDOW 10005
#define IDM_OPTION 10044
#define IDM_HELP 10045
IDR_MENU MENUEX
BEGIN
  POPUP "&File",IDM_FILE
  BEGIN
    MENUITEM "&New File",IDM_FILE_NEW
    MENUITEM "&Open File",IDM_FILE_OPEN
    MENUITEM "&Save File",IDM_FILE_SAVE
    MENUITEM "Save All Files",IDM_FILE_SAVEALL
    MENUITEM "",10009,0x00000800
    MENUITEM "&Print",IDM_FILE_PRINT
    MENUITEM "",10003,0x00000800
    MENUITEM "&Exit",IDM_FILE_EXIT
  END
  POPUP "&Edit",IDM_EDIT
  BEGIN
    MENUITEM "&Undo",IDM_EDIT_UNDO
    MENUITEM "Redo",IDM_EDIT_REDO
    MENUITEM "",10021,0x00000800
    MENUITEM "Cut",IDM_EDIT_CUT
    MENUITEM "&Copy",IDM_EDIT_COPY
    MENUITEM "&Paste",IDM_EDIT_PASTE
    MENUITEM "&Delete",IDM_EDIT_DELETE
    MENUITEM "",10016,0x00000800
    MENUITEM "&Find",IDM_EDIT_FIND
    MENUITEM "&Replace",IDM_EDIT_REPLACE
    POPUP "Block",IDM_EDIT_BLOCK
    BEGIN
      MENUITEM "Indent",IDM_EDIT_INDENT
      MENUITEM "Outdent",IDM_EDIT_OUTDENT
      MENUITEM "Comment",IDM_EDIT_COMMENT
      MENUITEM "Uncomment",IDM_EDIT_UNCOMMENT
    END
    POPUP "Bookmark",IDM_EDIT_BOKKMARK
    BEGIN
      MENUITEM "Toggle",IDM_EDIT_TOGGLEBM
      MENUITEM "Next",IDM_EDIT_NEXTBM
      MENUITEM "Previous",IDM_EDIT_PREVBM
      MENUITEM "Clear",IDM_EDIT_CLEARBM
    END
  END
  POPUP "&View",IDM_VIEW
  BEGIN
    POPUP "&Toolbar",IDM_VIEW_TOOLBAR
    BEGIN
      MENUITEM "&File",IDM_VIEW_TBFILE
      MENUITEM "&Edit",IDM_VIEW_TBEDIT
      MENUITEM "&Bookmark",IDM_VIEW_TBBOOKMARK
      MENUITEM "&View",IDM_VIEW_TBVIEW
      MENUITEM "&Make",IDM_VIEW_TBMAKE
      MENUITEM "Build Type",IDM_VIEW_TBBUILD
    END
    MENUITEM "&Statusbar",IDM_VIEW_STATUSBAR
    MENUITEM "&Project",IDM_VIEW_PROJECT
    MENUITEM "Properties",IDM_VIEW_PROPERTIES
    MENUITEM "&Output",IDM_VIEW_OUTPUT
    MENUITEM "Tab Select",IDM_VIEW_TAB
  END
  MENUITEM "&Format",IDM_FORMAT
  MENUITEM "&Project",IDM_PROJECT
  POPUP "&Make",IDM_MAKE
  BEGIN
    MENUITEM "&Assemble",IDM_MAKE_ASSEMBLE
    MENUITEM "&Build",IDM_MAKE_BUILD
    MENUITEM "&Run",IDM_MAKE_RUN
    MENUITEM "&Go",IDM_MAKE_GO
  END
  MENUITEM "&Tools",IDM_TOOLS
  MENUITEM "&Window",IDM_WINDOW
  MENUITEM "&Option",IDM_OPTION
  MENUITEM "&Help",IDM_HELP
END
