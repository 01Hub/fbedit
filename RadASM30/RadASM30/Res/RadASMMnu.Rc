#define IDM_EDIT_COPY 10013
#define IDM_EDIT_PASTE 10014
#define IDM_EDIT_DELETE 10015
#define IDM_EDIT_FIND 10017
#define IDM_EDIT_REPLACE 10018
#define IDM_EDIT_BLOCK 10022
#define IDM_EDIT_INDENT 10028
#define IDM_EDIT_OUTDENT 10029
#define IDM_EDIT_COMMENT 10030
#define IDM_EDIT_UNCOMMENT 10031
